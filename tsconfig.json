{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "outDir": "dist",
    "rootDir": "src",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowJs": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "preserveSymlinks": false,
    "allowSyntheticDefaultImports": true                         /* Skip type checking all .d.ts files. */
  },
  "include": ["src","upload"],
  "exclude": ["node_modules", "dist"] , 
}
